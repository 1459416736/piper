    "element-ui": "^1.2.4",
    "interactjs": "^1.2.8",
    "koa": "^2.1.0",
    "koa-bodyparser": "^4.1.0",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.0.0",
    "koa-session": "^5.1.0",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "mysql2": "^1.3.3",
    "nuxt": "^1.0.0-rc8",
    "passport-github": "^1.1.0",
    "pg": "^6.4.2",
