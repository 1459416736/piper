{
  "name": "piper",
  "version": "1.0.5",
  "description": "testing",
  "private": true,
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config build/webpack.prod.conf.js",
